{
    "name": "yii2mod/yii2-ftp",
    "type": "library",
    "description": "A flexible FTP and SSL-FTP client for PHP. This lib provides helpers easy to use to manage the remote files.",
    "license": "MIT",
    "keywords": [
        "yii2",
        "extension",
        "ftp",
        "sftp",
        "ssl-ftp",
        "ssl",
        "file",
        "server",
        "lib",
        "helper"
    ],
    "require": {
        "php": ">=5.4",
        "ext-ftp": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "yii2mod\\ftp\\": ""
        }
    }
}
